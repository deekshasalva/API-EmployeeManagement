@model IEnumerable<EmployeeManagement.Models.EmployeeModel>


<table class="table">
    <tr>
        <th>
            Employee Id
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.employeeId)
        </td>
        <td>
            @Html.ActionLink("Search", "GetEmployee", new {  id=item.employeeId })
        </td>
    </tr>
}

</table>
